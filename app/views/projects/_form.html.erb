<%= form_for Project.new do |form| %>


  <div class="field form-group">
    <%= form.label :Name  %>
    <%= form.text_field :name, id: :project_name , class:'form-control'%>
  </div>

  <div class="field">
    <%= form.label :skill %>
    <%= form.text_field :skill, id: :project_skill %>
  </div>

  <div class="field">
    <%= form.label :description %>
    <%= form.text_area :description, id: :project_descrtiption, class:'form-control' %>
  </div>
 </div><br>

  <div class="actions form-group">
    <%= form.submit 'Add Project' ,class:'btn btn-success' %>
  </div>
<% end %>
